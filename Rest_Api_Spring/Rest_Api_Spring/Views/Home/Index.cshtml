@using Rest_Api_Spring.models
@{
    ViewData["Title"] = "Home Page";
}
@model List<User>;





<table class="table table-dark">
    <thead>
        <tr>
            <th scope="col">id</th>
            <th scope="col">Nome</th>
            <th scope="col">Cognome</th>
            <th scope="col">Abbonamento</th>
            <th scope="col">Info</th>
        </tr>
    </thead>
    @foreach (User users in Model)
    {

        <tbody>
            <tr>
                <td>@users.UserId</td>
                <td>@users.FirstName</td>
                <td>@users.LastName</td>
                <td>@users.UserType</td>
                <td><a class="btn btn-primary" asp-action="Detail" asp-route-id="@users.UserId">Info</a></td>
            </tr>
        </tbody>


    }
</table>